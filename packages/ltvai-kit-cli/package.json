{
  "name": "ltvai-kit-cli",
  "version": "1.0.0",
  "description": "CLI tool to setup LTVAI Kit configuration for Claude Code and Antigravity",
  "type": "module",
  "bin": {
    "ltvai-kit-cli": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=esm --target=node20 --packages=external --banner:js=\"#!/usr/bin/env node\"",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "ltvai",
    "claude-code",
    "antigravity",
    "developer-tools",
    "configuration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cfonts": "^3.3.1",
    "commander": "^14.0.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.27.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist"
  ]
}
